<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".musicplayer" android:textAlignment="viewEnd" android:background="#05041A"
                                                   android:backgroundTintMode="add">
    <EditText
            android:layout_width="116dp"
            android:layout_height="20dp"
            android:focusableInTouchMode="false"
            android:text="End Time"
            android:ems="10"
            android:id="@+id/PlayerEndTime"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.898" app:layout_constraintVertical_bias="0.888"
            android:background="@null" android:textAlignment="textEnd" android:textColor="@color/white"
            android:fontFamily="@font/jetbrains_mono" android:textSize="12sp"/>
    <EditText
            android:layout_width="116dp"
            android:layout_height="20dp"
            android:focusableInTouchMode="false"
            android:inputType="text"
            android:text="Start Time"
            android:ems="10"
            android:id="@+id/PlayerStartTime"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.101" app:layout_constraintVertical_bias="0.888"
            android:background="@null" android:textAlignment="textStart" android:textColor="@color/white"
            android:fontFamily="@font/jetbrains_mono" android:textSize="12sp" android:editable="false"/>
    <SeekBar
            android:layout_width="351dp"
            android:layout_height="22dp" android:id="@+id/seekBar"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.919" app:layout_constraintHorizontal_bias="0.491"
            android:backgroundTint="@color/TimelineGray" android:backgroundTintMode="src_in"
            android:background="@drawable/playertimescrollbarbg" android:progressTint="#48B008"
            android:foregroundTint="#1E1E1E" android:progressBackgroundTint="#1E1E1E" android:thumbTint="#1E1E1E"/>
    <EditText
            android:layout_width="306dp"
            android:layout_height="40dp"
            android:focusableInTouchMode="false"
            android:inputType="text"
            android:text="Title"
            android:ems="10"
            android:id="@+id/PlayerSongTitle"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.47" app:layout_constraintVertical_bias="0.442"
            android:background="@color/TitleGreen" android:textColor="@color/black"
            android:fontFamily="@font/jetbrains_mono"
            android:textAlignment="center"
            android:textSize="24dp" android:textStyle="bold" android:editable="false"/>
    <EditText
            android:layout_width="308dp"
            android:layout_height="19dp"
            android:focusableInTouchMode="false"
            android:inputType="text"
            android:text="Subtitle"
            android:ems="10"
            android:id="@+id/PlayerSongSubtitle"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.485" app:layout_constraintVertical_bias="0.512"
            android:background="@color/TitleGreen" android:textColor="@color/black"
            android:fontFamily="@font/jetbrains_mono"
            android:textAlignment="center"
            android:textSize="14sp" android:editable="false"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/group_1" android:id="@+id/PlayerPlayButton"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintVertical_bias="0.744"
            android:background="@null"/>
    <ImageView
            android:layout_width="180dp"
            android:layout_height="180dp" android:id="@+id/imageView"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintVertical_bias="0.117" app:srcCompat="@android:drawable/ic_menu_report_image"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/playerprevbut" android:id="@+id/PlayerPrevBut"
            app:layout_constraintEnd_toStartOf="@+id/PlayerPlayButton"
            android:layout_marginEnd="19dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.729"
            android:background="@null"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/playernextbut" android:id="@+id/PlayerNextBut"
            app:layout_constraintStart_toEndOf="@+id/PlayerPlayButton"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.163"
            app:layout_constraintVertical_bias="0.729"
            android:background="@null"/>

</androidx.constraintlayout.widget.ConstraintLayout>